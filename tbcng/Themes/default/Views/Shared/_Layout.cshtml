<!DOCTYPE html>
<html lang="en">
<head prefix="og: http://ogp.me/ns#">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@Html.Raw(ViewBag.Title)</title>
    @RenderSection("metatags", false)

    <!-- Favicons
    ================================================== -->
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" type="image/png" href="/favicon.png" />

    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/bootstrap.css")" rel="stylesheet">
    <!-- Jquery Range Slider CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/range-slider.css")" rel="stylesheet">
    <!-- Owl Carousel CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/owl.carousel.css")" rel="stylesheet">
    <!-- Pretty Photo CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/prettyPhoto.css")" rel="stylesheet">
    <!-- Bx-Slider StyleSheet CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/jquery.bxslider.css")" rel="stylesheet">
    <!-- Font Awesome StyleSheet CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/font-awesome.css")" rel="stylesheet">
    <!-- SVG Font StyleSheet CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/svg.css")" rel="stylesheet">
    <!-- Chosen Script CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/chosen.min.css")" rel="stylesheet">
    <!-- DL Menu CSS -->
    <link href="@Url.Content("~/Themes/default/Content/js/dl-menu/component.css")" rel="stylesheet">
    <!-- Widget CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/widget.css")" rel="stylesheet">
    <!-- Typography CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/typography.css")" rel="stylesheet">
    <!-- Shortcodes CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/shortcodes.css")" rel="stylesheet">
    <!-- Custom Main StyleSheet CSS -->
    <link href="@Url.Content("~/Themes/default/Content/style.css")" rel="stylesheet">
    <!-- Color CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/color.css")" rel="stylesheet">
    <!-- Responsive CSS -->
    <link href="@Url.Content("~/Themes/default/Content/css/responsive.css")" rel="stylesheet" />
    <!--Jquery Library-->
        @RenderSection("styles", false)
        <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    @*<script src="@Url.Content("~/Themes/default/Content/js/jquery.js")"></script>*@
    @RenderSection("scripts2", required: false)
</head>

<body>

    <!--Kode Wrapper Start-->
    <div class="kf_wrapper">
        @Html.Partial("_HeaderPartial")
        
        <!--Main Banner Wrap Start-->
        @RenderSection("MainBanner", false)
        <!--Main Banner Wrap End-->

        <!--Content Wrap Start-->
        <div class="kf_property_content_wrap">
            @RenderBody()
        </div>
        <!--Content Wrap End-->
    </div>
    
    @Html.Partial("_FooterPartial")
    
    <!--Kode Wrapper End-->
    <!--Jquery UI Library-->
    <script src="@Url.Content("~/Themes/default/Content/js/range-slider.js")"></script>
    <!--Bootstrap core JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/bootstrap.js")"></script>
    <!--Bx-Slider JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/jquery.bxslider.js")"></script>
    <!--Owl Carousel JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/owl.carousel.js")"></script>
    <!--Pretty Photo JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/jquery.prettyPhoto.js")"></script>
    <!--Accordian JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/jquery.accordion.js")"></script>
    <!--Number Count (Waypoints) JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/waypoints-min.js")"></script>
    <!-- Chosen Script Javascript -->
    <script src="@Url.Content("~/Themes/default/Content/js/chosen.jquery.min.js")"></script>
    <!-- Time Counter Script Javascript -->
    <script src="@Url.Content("~/Themes/default/Content/js/jquery.downCount.js")"></script>
    <!--Dl Menu Script-->
    <script src="@Url.Content("~/Themes/default/Content/js/dl-menu/modernizr.custom.js")"></script>
    <script src="@Url.Content("~/Themes/default/Content/js/dl-menu/jquery.dlmenu.js")"></script>
    <!--Map JavaScript-->
    <!--Custom JavaScript-->
    <script src="@Url.Content("~/Themes/default/Content/js/custom.js")"></script>
    @RenderSection("scripts", required: false)
    @Html.Partial("_script_googleanalystic")


</body>
</html>
